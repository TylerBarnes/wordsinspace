teaching: allWpPost(filter: {
  categories: {nodes: {elemMatch: {name: {eq: "Teaching"}}}}}) 
{
  totalCount
  edges {    
    node {
      slug
      link
      categories {
        nodes {
          name
        }
      }
    }
  }
}

